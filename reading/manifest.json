{
    "manifest_version": 3,
    "name": "reading",
    "version": "0.1.0",
    "description": "A simple extension that reads the current page",
    "icons" : {
        "16": "images/thumb16.png",
        "48": "images/thumb48.png",
        "32": "images/thumb32.png",
        "128": "images/thumb128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content.js"]
        }
    ]
}